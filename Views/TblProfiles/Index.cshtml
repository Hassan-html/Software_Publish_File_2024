@model IEnumerable<SialkotSweet.Models.TblProfile>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid">
    <h2>Software Profile Enteries</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Navbar_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dashboard_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Receipt_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MobileNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Details)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Software_Company)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_Contact)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Navbar_Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Dashboard_Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Receipt_Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Address)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.MobileNo)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Phone)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Details)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Software_Company)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Company_Contact)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

    </table>
</div>
